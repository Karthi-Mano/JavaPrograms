
public class Demo
{
    int a=100;
	void method1()
	{
		String name="niit";
		System.out.println("Method-1"+a);
	}
	void method2()
	{
		System.out.println("Method-2"+a);
	}
	
}	

class MainClass
{
	public static void main(String[] args)
	{
		
	  Demo g1=new Demo();//To create a object
	  g1.method1();
	  g1.method2();

	}

}
