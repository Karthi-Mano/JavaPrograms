
public class Demo1 extends Thread
{
	public void run()
	{
		try
		{
		for(int i=0;i<5;i++)
		{
			
			wait();
		}
		 }
		catch(Exception g)
		{
			System.out.println(g);
		}
	}
	synchronized void get1()
	{
		System.out.println("MethodIN");
		notify();
		System.out.println("MethodOUT");
		
	}
}
class Foo
{
	public static void main(String[] args) 
	{
		Demo1 t1=new Demo1();
        //Demo1 t2=new Demo1();
        t1.start();

	}

}
