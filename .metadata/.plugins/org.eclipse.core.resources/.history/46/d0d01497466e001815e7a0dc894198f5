
public class Demo {

	public static void main(String[] args) 
	{
		int i, j, k;
	    for(i=1;i<=5;i++)
	    {
	    	System.out.print("i value is"+i);
	        for(j=i;j<5;j++)
	        {
	           System.out.print(" ");
	        }
	        for(k=1;k<(i*2);k++)
	        {
	               System.out.print("*");
	        }
	        System.out.print("\n");
	    }	

	}

}
