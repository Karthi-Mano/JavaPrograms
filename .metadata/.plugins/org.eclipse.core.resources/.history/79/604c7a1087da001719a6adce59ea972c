
public class Demo 
{
	int cid=100;//class variables
   void g1()
   {
	   int pid=200;//local variables
	   System.out.println("welcome m1"+cid+"product id is"+pid);
   }
   void g2()
   {
	   System.out.println("welcome m2"+cid);
   }
}
class Demo1
{

	public static void main(String[] args)
	{
		Demo g=new Demo();
		g.g2();
		g.g1();

	}

}
