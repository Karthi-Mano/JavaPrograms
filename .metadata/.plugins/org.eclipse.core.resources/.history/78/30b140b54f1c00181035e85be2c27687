
public class Demo 
{
	
	int a;//class or instance variable
	Scanner k=new Scanner(System.in);
	void g1()
	{
		int b;//local variable
		System.out.println("welcome to "+b+""+a);
	}
	void g2()
	{
		System.out.println("welcome to"+a);
	}

	public static void main(String as[])
	{
		Demo g=new Demo();
		
		g.g1();
		g.g2();

	}

}
