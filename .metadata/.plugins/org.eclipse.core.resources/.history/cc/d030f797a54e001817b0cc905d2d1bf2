
public class Demo extends Thread
{

	public void run()
	{
		try
		{
			System.out.println("IN");
			sleep(3000);
			System.out.println("OUT");
		}
		catch(InterruptedException t)
		{
			
		}
	}
	
	public static void main(String[] args) 
	{
		Demo t1=new Demo();
		Demo t2=new Demo();
		t1.start();
		t2.start();
		

	}

}
