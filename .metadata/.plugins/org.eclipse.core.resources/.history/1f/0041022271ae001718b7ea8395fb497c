
public class Demo1 extends Thread
{
	public void run()
	{
		for(int i=0;i<5;i++)
		{
			try
			{
			wait(3000);
			}
			catch(Exception g)
			{
				
			}
		}
	}
}
class Foo
{
	public static void main(String[] args) 
	{
		Demo1 t1=new Demo1();
        Demo1 t2=new Demo1();
        t1.start();

	}

}
