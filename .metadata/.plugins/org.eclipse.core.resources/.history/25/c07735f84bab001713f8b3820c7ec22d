package Customer;

import java.util.*;


public class Customer 
{
   public String cusName;
   public int cusId,age,mobile;
   Scanner h=new Scanner(System.in);
   HashSet<String> hs=new HashSet<>();
   ArrayList<>
   public void addCustomer()
   {
	   System.out.println("Enter the Customer Details:\n");
	   cusName=h.nextLine();
	   cusId=h.nextInt();
	   age=h.nextInt();
	   mobile=h.nextInt();
	   hs.add(cusName);
	   hs.add(cusId);
   }
   
}
