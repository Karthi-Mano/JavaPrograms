
public class Demo extends Thread
{
	
  public void run()
  {
	  try
	  {
	  Thread.sleep(3000);
	  }
	  catch(InterruptedException r)
	  {
		  System.out.println(r);
	  }
  }
  void run1()
  {
	  System.out.println("Normal method");
  }
}
class Foo
{
	public static void main(String[] args) 
	{
		Demo t1=new Demo();
		t1.start();
		t1.run1();

	}

}
