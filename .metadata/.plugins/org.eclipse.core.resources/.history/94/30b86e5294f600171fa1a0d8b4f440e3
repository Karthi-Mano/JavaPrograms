
public class Demo extends Thread
{
	
	public void run()
	{
		try
		{
		System.out.println("welcome-1");
		Thread.sleep(2000);
		}
		catch(InterruptedException t)
		{
			System.out.println(t);
		}
	}
	void g2()
	{
		System.out.println("welcome-2");
	}
}
class Foo
{
	public static void main(String[] args)
	{
		Demo h=new Demo();
		h.start();
		h.g2();

	}

}
