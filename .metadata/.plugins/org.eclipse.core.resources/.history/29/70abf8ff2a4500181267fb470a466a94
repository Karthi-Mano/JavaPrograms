
public class Demo
{

	Demo()//default cons
	{ 
		final int a1=200;
		//a1++;
		System.out.println("welcome"+a1);
	}
	Demo(int a)//param cons
	{
		a1=300;
		System.out.println("data is"+a);
	}
	
}
class Sample
{
	public static void main(String[] args) 
	{
           new Demo();//default invoke
         
           new Demo(300);

	}

}
