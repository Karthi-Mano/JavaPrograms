import java.util.*;

public class ScannerExample 
{
  String uname;
  int pid;
  float f1;
  Scanner g1=new Scanner(System.in);
  void input()
  {
	  System.out.println("Enter the value is:\n");
	  uname=g1.next();
	  pid=g1.nextInt();
	  f1=g1.nextFloat();
  }
}
