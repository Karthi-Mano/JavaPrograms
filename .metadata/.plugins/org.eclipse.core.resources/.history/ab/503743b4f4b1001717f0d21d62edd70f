
public class StaticExample 
{
	static int a=100;
    public StaticExample() 
    {
    	a++;
    	System.out.println("Value is:"+a);
	}
    public StaticExample(int studentid)
    {
    	System.out.println("YourStudent id is:\n"+studentid);
    }
    
	public static void main(String[] args) 
	{
		new StaticExample();
		new StaticExample();

	}

}
