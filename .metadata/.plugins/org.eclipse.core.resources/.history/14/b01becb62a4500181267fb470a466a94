
public class Demo
{

	Demo()//default cons
	{ 
		int a1=10;
		a1++;
		System.out.println("welcome"+a1);
	}
	Demo(int a)//param cons
	{
		System.out.println("data is"+a);
	}
	
}
class Sample
{
	public static void main(String[] args) 
	{
           new Demo();//default invoke
           new Demo();
           new Demo(300);

	}

}
