
public class Demo extends Thread
{

	public void run()
	{
		try
		{
			System.out.println("IN");
			sleep(3000);
			System.out.println("OUT");
		}
		catch(InterruptedException t)
		{
			
		}
	}
	public void run()
	{
		System.out.println("***");
	}
	
	public static void main(String[] args) 
	{
		Demo g=new Demo();
		g.start();

	}

}
