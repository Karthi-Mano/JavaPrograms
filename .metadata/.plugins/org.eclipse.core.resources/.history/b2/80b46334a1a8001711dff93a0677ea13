package Salary;

public class CalSalary
{
  private int  empid,empname,basic,hra,da;

public int getEmpid() {
	return empid;
}

public void setEmpid(int empid) {
	this.empid = empid;
}

public int getEmpname() {
	return empname;
}

public void setEmpname(int empname) {
	this.empname = empname;
}

public int getBasic() {
	return basic;
}

public void setBasic(int basic) {
	this.basic = basic;
}

public int getHra() {
	return hra;
}

public void setHra(int hra) {
	this.hra = hra;
}

public int getDa() {
	return da;
}

public void setDa(int da) {
	this.da = da;
}
  public void calsalary()
  {
	  
  }
}
