import java.util.Scanner;

public class SingleDimArray 
{
  int p[]=new int[20];
  Scanner g=new Scanner(System.in);
  int n,i,k;
  void g1()
  {
	  System.out.println("Enter the n value is:\n");
	  n=g.nextInt();
  }
  void g2()
  {
	  for(i=0;i<=n;i++)
	  {
		  System.out.println("Enter the array value is:\n");
		  p[i]=g.nextInt();
		  //System.out.println("Result is:\n"+p[i]);
	  }
	  for(i=0;i<=n;i++)
	  {
		  System.out.println("Result is:\n"+p[i]);
	  }
	  System.out.println("Enter the index value is:\n");
	  k=g.nextInt();
	  
	  for(k=0;k<=n;k++)
	  {
		  if(k==p[i])
		  {
			  System.out.println("your index value is:"+p[i]);
		  }
		  else
		  {
			  System.out.println("Not found..");
		  }
	  }
	  
	  
  }
}
class Sample
{
	public static void main(String[] args) 
	{
		SingleDimArray g=new SingleDimArray();
		g.g1();
		g.g2();

	}

}
