
public class Demo 
{
	int p;//class variables
   void g1(int p)
   {
	   //int p=cid;//local variables
	   System.out.println("product id is"+p);
   }
   void g2(String name)
   {
	   System.out.println("welcome m2"+name);
   }
}
class Demo1
{

	public static void main(String[] args)
	{
		Demo g=new Demo();
		g.g2("niit");
		g.g1(100);

	}

}
