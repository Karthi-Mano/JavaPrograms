

class Demo1
{
	
	Scanner h=new Scanner(System.in);
	int pid;
	float f1;
	String u1;
   void get1()
   {
	   System.out.println("Enter the value is:\n");
	   pid=h.nextInt();
	   f1=h.nextFloat();
	   u1=h.next();
   }
   void get2()
   {
	   System.out.println("Your data is:\n"+pid+""+f1+""+u1);
   }
}
class Foo
{
	public static void main(String[] args) 
	{
		Demo1 g=new Demo1();
		g.get1();
		g.get2();
	}

}
