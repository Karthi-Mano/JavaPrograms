
public class Demo extends Thread
{
 public void run()
  {
	 System.out.println("ThreadName is::"+Thread.currentThread().getName());
	  
  }
}
class Sample
{
	public static void main(String[] args) 
	{
	   Demo t1=new Demo();
	   Demo t2=new Demo();
	   
	   t1.setName("NIIT");
	   t1.start();
	   t2.setPriority(Thread.MIN_PRIORITY);
	   t2.start();

	}

}
