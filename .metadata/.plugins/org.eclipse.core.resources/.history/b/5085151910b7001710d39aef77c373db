import java.util.ArrayList;

public class Demo 
{

	public static void main(String[] args) 
	{
      ArrayList list=new ArrayList();
      list.add("mohan");
      list.add("raja");
      list.add("sheik");
      System.out.println("Result is:\n"+list);
      System.out.println("TotalSize is:"+list.size());
	}

}
