package Customer;

import java.util.*;


public class Customer 
{
   public String cusName[]=new String[5];
   public int cusId[]=new int[4];
   public int age[]=new int[5];
   public int mobile[]=new int[5];
   Scanner h=new Scanner(System.in);
   HashSet<String> hs=new HashSet<>();
   ArrayList<Integer> as=new ArrayList<>();
   public void addCustomer()
   {
	   System.out.println("Enter the Customer Details:\n");
	   for(int i=0;i<2;i++)
	   {
	   cusName[i]=h.next();
	   cusId[i]=h.nextInt();
	   age[i]=h.nextInt();
	   mobile[i]=h.nextInt();
	  
	   }
	   for(int i=0;i<2;i++)
	   {
	   hs.add(cusName[i]);
	   as.add(cusId[i]);
	   as.add(age[i]);
	   as.add(mobile[i]);
	   }
	   
	    Iterator j=hs.iterator();
	    Iterator j1=as.iterator();
	    while(j.hasNext()&&j1.hasNext())
	    {
	      System.out.println("Your value is:\n"+j.next());
	      System.out.println("Your Value is:\n"+j1.next());
	    }
	  
   }
   
}
